.gnbArea .gnbBox .saleArea .sale:hover { background:url('../images/common/btn/btn_sale_on.gif') no-repeat;}
.gnbArea .gnbBox .saleArea .basket { display:block; width: 68px; height: 26px; border:none; background:url('../images/common/btn/btn_basket.gif') no-repeat; }
