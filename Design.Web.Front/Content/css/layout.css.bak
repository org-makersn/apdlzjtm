.gnbArea .gnbBox { width:1000px; margin:0 auto; overflow: hidden; }
.gnbArea .gnbBox .nav { float:left; overflow: hidden; }
.gnbArea .gnbBox .nav li { float:left; color:#575757; margin:15px 20px 12px 0; font-size: 13px; }
.gnbArea .gnbBox .nav li a:hover { color:#ff7900;}
.gnbArea .gnbBox .nav li.on { color:#ff7900; }
.gnbArea .gnbBox .nav li.on a { color:#ff7900; }
.gnbArea .gnbBox .saleArea { float:right; }
.gnbArea .gnbBox .saleArea li { float: left; margin: 7px 0 7px 10px }
