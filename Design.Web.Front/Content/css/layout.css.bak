.gnbArea .gnbBox .saleArea .sale { display:block; width: 70px; height: 26px; background-color: #fff; color: #ff7900; border: 1px solid #ff7900; }
.gnbArea .gnbBox .saleArea .sale:hover { background-color: #ff7900; color: #fff; }
.gnbArea .gnbBox .saleArea .basket { display:block; width: 70px; height: 26px; background-color: #fff; color: #ff7900; border: 1px solid #ff7900; }
.gnbArea .gnbBox .saleArea .basket:hover { background-color: #ff7900; color: #fff; }

.gnbArea .gnbBox .saleArea .btnR { display:block; width: 70px; height: 26px; background-color: #fff; color: #ff7900; border: 1px solid #ff7900; }
.gnbArea .gnbBox .saleArea .btnR:hover { background-color: #ff7900; color: #fff; }


.gnbArea .gnbBox .uploadArea { float:right; margin: 7px 0 7px 10px; overflow: hidden; display: inline-block; }
.gnbArea .gnbBox .uploadArea .btnSet { float: left; overflow: hidden; margin-right: 10px; }
.gnbArea .gnbBox .uploadArea .btnSet li { float: left; }
.gnbArea .gnbBox .uploadArea .upload { float: left; width: 65px; height: 26px; background-color: #fff; color: #ff7900; border: 1px solid #ff7900; }
.gnbArea .gnbBox .uploadArea .upload:hover { width: 65px; height: 26px; background-color: #ff7900; color: #fff; }

/* TopVisual */
.topVisu { background:url('../images/store_bg1.jpg') repeat-x; width:100%; height:400px; margin-bottom: 15px; text-align:center; }
.topVisu img { height:400px; }
/* MainSlide */
.unslider-carousel	{position:relative;overflow:auto;background:#4FC9FF;display:block;height:400px;width:100%; }
.unslider-carousel ul	{list-style:none;margin:0;width:100%}
.unslider-carousel ul li	{float:left; height:400px;width:100%;padding:0;margin:0;display:block;position:relative}
.unslider-carousel .container	{margin:0 auto;position:relative;top:80%;-webkit-transform:translateY(-80%);-ms-transform:translateY(-80%);transform:translateY(-80%)}
.unslider-carousel .container p.txt1 { font-family: Roboto; font-size: 42px; color:#fff; text-align: center; }
.unslider-carousel .dots {position:absolute; left:0; right:0; bottom:20px; margin:0; text-align:center; line-height:1em}
.unslider-carousel .dots li{display:inline-block; width:13px; height:13px; margin:0 4px; text-indent:-999em; border-radius:10px; cursor:pointer; opacity:1; background:#fff; }
.unslider-carousel .dots li.active{background:#ff7900; opacity:1}
.attribution	{position:absolute;z-index:5;bottom:17px;right:20px;color:#edeff0;color:rgba(255,255,255,0.7);font-size:11px;text-decoration:none}
.attribution a{text-rendering:optimizeLegibility;color:rgba(255,255,255,0.7)}
.attribution a:hover,.attribution a:active{color:#fff}
*{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box}

/* Main */
.contWrap { width:1000px; margin:0 auto; overflow:hidden; }
.contWrap .mainText { margin: 30px 0 60px 148px; }
.contWrap .mainTitle { margin: 30px 0 10px 0; overflow:hidden; }
.contWrap .mainTitle h2 { float:left; color: #333; font-size: 19px; }
.contWrap .mainTitle p { float:right; padding-top: 10px; }
.contWrap .mainTitle p a { font-size:11px; color: #575757; }
.contWrap .mainTitle .sMenu { float: right; overflow: hidden; }
.contWrap .mainTitle .sMenu li { float: left; padding-left: 22px; font-size: 14px; color: #555; }
.contWrap .mainTitle .sMenu li a { color: #555; }
.contWrap .mainTitle .sMenu li a:hover { color: #ff7900; }
.contWrap .mainTitle .sMenu li.on, .contWrap .mainTitle .sMenu li.on a { color: #ff7900; }
.contWrap .subtitle { border-bottom: 1px solid #dedede; padding: 18px 0 12px 0; overflow: hidden; }
.contWrap .subtitle h2 { float: left; font-size: 19px; color:#575757; }
.contWrap .subtitle a { float: right; }

/* Info */
