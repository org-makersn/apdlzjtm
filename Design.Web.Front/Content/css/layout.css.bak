/* Wrap */
.wrap { width:100%; height:100%; overflow-x: hidden; position: relative; background: #f2f2f2; }
.wrap.bgW { background: #fff !important; }

/* Header */
.header { width:100%; overflow: hidden; position: relative; }
.header .headerTop { top:0; left:0; width:100%; background: #3b3b3b; overflow: hidden; z-index:2; }
.header .headerBox { width:1000px; height: 50px; margin:0 auto; overflow: hidden; }
.header .headerBox h1 { text-align: center; margin: 10px 0; }
.header .headerBox .mainMenu { float: left; margin-top: -33px;}
.header .headerBox .mainMenu li { float: left; color: #fff; font-weight: bold; font-size: 13px; margin-right: 45px; }
.header .headerBox .mainMenu li a { color: #fff; font-weight: bold; }
.header .headerBox .mainMenu li a:hover { color: #ff7f10; font-weight: bold; }
.header .headerBox .mainMenu li .on { color: #ff7f10; }
.header .headerBox .rightZone { float:right; margin-top: -40px; overflow:hidden; }
.header .headerBox .rightZone .searchArea { float:left; width:215px; margin-right: 30px; }
.header .headerBox .rightZone .searchAre .on { border-bottom: 1px solid #aaa; }
.header .headerBox .rightZone .searchArea input{ border:none; width:180px; height:15px;  outline:none; background-color: #3b3b3b; }
.header .headerBox .rightZone .searchArea button{ border:none; width:30px; height:30px; background:url('../images/common/btn/btn_search.png') no-repeat;}
.header .headerBox .rightZone .memberArea { float:left; width:30px; }
.header .headerBox .rightZone .en { position:absolute; top: 16px; left: 50%; margin-left: 535px; }

.topMypage { display:none; position:absolute; width: 140px; top:50px; left:50%; margin-left:360px; background-color: #3b3b3b; color: #fff; z-index: 2 !important;}
.topMypage ul {  overflow:hidden; }
.topMypage ul li { padding: 15px 0; width: 140px; text-align:center; border-bottom:1px solid #5e5e5e; font-size: 13px; }
.topMypage ul li a { color: #fff; }
.header .headerBox .rightZone .en { position:absolute; top: 16px; left: 50%; margin-left: 535px; }

/* GnbArea */
.gnbArea { width:100%; top:50px; left:0; width:100%; background: #fff; overflow: hidden; z-index:2; }
.gnbArea .gnbBox { width:1000px; margin:0 auto; overflow: hidden; }
.gnbArea .gnbBox .nav { float:left; overflow: hidden; }
.gnbArea .gnbBox .nav li { float:left; color:#575757; margin:15px 20px 12px 0; }
.gnbArea .gnbBox .nav li a:hover { color:#ff7900;}
.gnbArea .gnbBox .nav li.on { color:#ff7900; }
.gnbArea .gnbBox .nav li.on a { color:#ff7900; }
.gnbArea .gnbBox .saleArea { float:right; }
.gnbArea .gnbBox .saleArea li { float: left; margin: 7px 0 7px 10px }
.gnbArea .gnbBox .saleArea .sale { display:block; width: 86px; height: 26px; border:none; background:url('../images/common/btn/btn_sale.gif') no-repeat; }
.gnbArea .gnbBox .saleArea .sale:hover { width: 86px; height: 26px; border:none; background:url('../images/common/btn/btn_sale_on.gif') no-repeat; }
.gnbArea .gnbBox .saleArea .basket { display:block; width: 86px; height: 26px; border:none; background:url('../images/common/btn/btn_basket.gif') no-repeat; }
.gnbArea .gnbBox .saleArea .basket:hover { width: 86px; height: 26px; border:none; background:url('../images/common/btn/btn_basket_on.gif') no-repeat; }
.gnbArea .gnbBox .uploadArea { float:right; margin: 7px 0 7px 10px; overflow: hidden; display: inline-block; }
.gnbArea .gnbBox .uploadArea .btnSet { float: left; overflow: hidden; margin-right: 10px; }
.gnbArea .gnbBox .uploadArea .btnSet li { float: left; }
.gnbArea .gnbBox .uploadArea .upload { float: left; width: 86px; height: 26px; border:none; background:url('../images/common/btn/btn_upload.gif') no-repeat; }
.gnbArea .gnbBox .uploadArea .upload:hover { width: 86px; height: 26px; border:none; background:url('../images/common/btn/btn_upload_on.gif') no-repeat; }

/* TopVisual */
.topVisu { background:url('../images/store_bg1.jpg') repeat-x; width:100%; height:433px; text-align:center; }

/* MainSlide */
.unslider-carousel	{position:relative;overflow:auto;background:#4FC9FF;display:block;height:430px;width:100%; }
.unslider-carousel ul	{list-style:none;margin:0;width:100%}
.unslider-carousel ul li	{float:left; height:430px;width:100%;padding:0;margin:0;display:block;position:relative}
.unslider-carousel .container	{margin:0 auto;position:relative;top:80%;-webkit-transform:translateY(-80%);-ms-transform:translateY(-80%);transform:translateY(-80%)}
.unslider-carousel .container p.txt1 { font-size:32px; color:#fff; font-weight:400; text-align: center; }
.unslider-carousel .dots {position:absolute; left:0; right:0; bottom:20px; margin:0; text-align:center; line-height:1em}
.unslider-carousel .dots li{display:inline-block; width:13px; height:13px; margin:0 4px; text-indent:-999em; border-radius:10px; cursor:pointer; opacity:1; background:#fff; }
.unslider-carousel .dots li.active{background:#ff7900; opacity:1}
.attribution	{position:absolute;z-index:5;bottom:17px;right:20px;color:#edeff0;color:rgba(255,255,255,0.7);font-size:11px;text-decoration:none}
.attribution a{text-rendering:optimizeLegibility;color:rgba(255,255,255,0.7)}
.attribution a:hover,.attribution a:active{color:#fff}
*{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box}

/* Main */
.contWrap { width:1000px; margin:0 auto; min-height:1100px; overflow:hidden; }
