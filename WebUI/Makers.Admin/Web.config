<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <!--<add name="MakersNContext" providerName="System.Data.SqlClient" connectionString="Server=storedb_dev;Database=dbluckymun;User ID=makers;Password=1234;Connection Timeout=30;" />-->
    <add name="MakersNContext" providerName="System.Data.SqlClient" connectionString="Server=makersndb_real;Database=dbluckymun;User ID=luckymun;Password=pingoli1234;Connection Timeout=30;" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    <add key="DBServerIP" value="storedb_dev" />
    <add key="DBUser" value="makers" />
    <add key="DBPwd" value="1234" />
    <add key="Database" value="dbluckymun" />
    

    <!--<add key="DBServerIP" value="makersndb_real" />
    <add key="DBUser" value="luckymun" />
    <add key="DBPwd" value="pingoli1234" />
    <add key="Database" value="dbluckymun" />-->
    
    <!--경로설정-->
    <add key="articleimgurl" value="http://local.makersn.com/fileupload/article/article_img/" />
    <add key="adminimgurl" value="http://local.makersn.com/fileupload/" />
    <add key="Article3DUrl" value="http://local.makersn.com/fileupload/article/article_3d/" />
    <add key="profileimgurl" value="http://local.makersn.com/fileupload/profile/thumb/" />
    <add key="msgImgThumb" value="http://local.makersn.com/fileupload/msg_file/thumb/" />
    <add key="msgImgOri" value="http://local.makersn.com/fileupload/msg_file/backup/" />
    <add key="PrinterImgUrl" value="http://local.makersn.com/fileupload/printer/printer_img/" />
    <add key="BannerUrl" value="http://local.makersn.com/fileupload/banner/" />
