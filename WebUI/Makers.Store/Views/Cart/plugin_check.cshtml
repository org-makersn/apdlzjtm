<!-------------------------------------------------------------------------------
* 										*
* 아래 자바 스크립트 함수(checkPlugin)는 고객 PC에 플러그인이			*
* 정상적으로 설치됐는지 체크하는 함수 입니다.					*
* 정상적으로 설치됐다면, 다음 결제 요청 페이지(INIsecurepay.html)를 호출하도록	* 
* 되어 있습니다. 								*
* 										*
* 웹SITE 가 https를 이용하면 https://plugin.inicis.com/pay40_check_ssl.js를 사용 
* 웹SITE 가 unicode를 이용하면 http://plugin.inicis.com/pay40_check_uni.js를 사용
* 웹SITE 가 https, unicode를 이용하면 https://plugin.inicis.com/pay40_check_unissl.js 사용  
*
-------------------------------------------------------------------------------->
<script type="text/JavaScript" src="http://plugin.inicis.com/pay40_check.js"></script>
<script type="text/JavaScript">
    if (navigator.userAgent.indexOf("Windows NT 6.0") != -1) {
        document.body.innerHTML = othird.innerHTML;
    }
    else {
            var g_fIsSP1 = false;
            g_fIsSP1 = (window.navigator.appMinorVersion.indexOf("SP2") != -1);
            if (g_fIsSP1) {
                document.body.innerHTML = ofirst.innerHTML;
            }
            else {
                document.body.innerHTML = osecond.innerHTML;
            }        
    }
    StartSmartUpdate();
    function checkPlugin() {
        if (navigator.appName == 'Netscape') {
            if (document.INIpay == null) {
                alert(" 자동으로 플러그인 프로그램이 설치되지 않았습니다. \n\n 페이지상의 조치절차에 따라 설치하세요.");
            }
            else {
                var url = "../order/list";
                var f = document.forms.cartInfo;

                f.action = url;
                f.submit();
            }
        }
        else {
            if (document.INIpay.object == null) {
                alert(" 자동으로 플러그인 프로그램이 설치되지 않았습니다. \n\n 페이지상의 조치절차에 따라 설치하세요.");
            }
            else {
                var url = "../order/list";
                var f = document.forms.cartInfo;

                f.action = url;
                f.submit();
            }
        }
    }


</script>
<script type="text/JavaScript">
    setTimeout("checkPlugin()", "1000");
</script>
<form name="cartInfo"  method="post">
    <input type="hidden" name="cartNo" value="@Model.CART_NO" />
</form>
