/*-------------------------------------------------------------------------------------------------------
* 1. ResultCode
*       가. 결 과 코 드: "00" 인 경우 결제 성공[무통장입금인 경우 - 고객님의 무통장입금 요청이 완료]	*
*       나. 결 과 코 드: "00"외의 값인 경우 결제 실패  						*
*------------------------------------------------------------------------------------------------------*/
	
function receipt(tid)
{
	if("@Model.Common.Resultcode" == "00")
	{
		var receiptUrl = "https://iniweb.inicis.com/DefaultWebApp/mall/cr/cm/mCmReceipt_head.jsp?noTid=" + @Model.Common.Tid + "&noMethod=1";
		window.open(receiptUrl,"receipt","width=430,height=700, scrollbars=yes,resizable=yes");
	}
	else
	{
		alert("해당하는 결제내역이 없습니다");
	}
}
	
function errhelp()
{
	var errhelpUrl = "http://www.inicis.com/Help/errorcode_list.jsp?pstPayMethod=all&pstCode=" + @Model.Common.ResultErrorcode
	window.open(errhelpUrl,"errhelp","width=573,height=423");
}
</script>

