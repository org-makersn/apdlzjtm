@{
    string backgroundUrl = "";

    if (Model.Common.Resultcode.Equals("01"))
    {
        backgroundUrl = "../content/images/payment/spool_top.gif";
    }
    else
    {
        switch (Model.Common.PayMethod)
        {
            case "Card": // 신용카드
                backgroundUrl = "../content/images/payment/card.gif";
                break;

            case "VCard":  // ISP
                backgroundUrl = "../content/images/payment/card.gif";
                break;
            case "HPP":  // 휴대폰
                backgroundUrl = "../content/images/payment/hpp.gif";
                break;

            case "Ars1588Bill":  // 1588
                backgroundUrl = "../content/images/payment/phone.gif";
                break;

            case "PhoneBill":  // 폰빌
                backgroundUrl = "../content/images/payment/phone.gif";
                break;
            case "OCBPoint":  // OKCASHBAG
                backgroundUrl = "../content/images/payment/okcash.gif";
                break;

            case "DirectBank":   // 은행계좌이체
                backgroundUrl = "../content/images/payment/bank.gif";
                break;

            case "VBank":   // 무통장 입금 서비스
                backgroundUrl = "../content/images/payment/bank.gif";
                break;

            case "Culture":   // 문화상품권 결제
                backgroundUrl = "../content/images/payment/ticket.gif";
                break;

            case "NEMO":   // 네모 결제
                backgroundUrl = "../content/images/payment/nemo.gif";
                break;

            case "Teen":   // 틴캐쉬
                backgroundUrl = "../content/images/payment/teen.gif";
                break;

            default:  // 기타 지불수단의 경우
                backgroundUrl = "../content/images/payment/card.gif";
                break;

        }
    }
    
}

